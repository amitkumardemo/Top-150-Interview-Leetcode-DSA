class Solution {
    public int removeDuplicates(int[] nums) {
        // if(nums.length == 0){
        //     return 0;
        // }
        // int j = 1; // store unique value

        // for(int i = 1; i < nums.length; i++){
        //     if(nums[i] != nums[i - 1]){
        //         nums[j++] = nums[i];
        //     }
        // }
        // return j;

        if(nums.length == 0){
            return 0;
        }
        int j = 1; // for stroing unique values
        for(int i = 1; i < nums.length; i++){
            if(nums[i] != nums[i-1]){
                nums[j++] = nums[i];
            }
        }
        return j;
    }
}
