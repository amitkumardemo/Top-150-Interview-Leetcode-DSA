class Solution {
    public int jump(int[] arr) {
     int canJumb = 0;
		int i = 0; 
		int start = 0;
		int count  = 0;
		while(i < arr.length-1)
		{
			int maxJumb = 0;
			int ind = 0;
			for(int j = start; j < arr.length && canJumb > 0; j++)
			{
				if(maxJumb < arr[j] + j) {
					maxJumb = arr[j] + j;
					ind = j;
				}
				canJumb--;
			}
			start = ind + 1;
			canJumb = arr[ind];
			i = arr[ind] + ind;
			count++;
		}
        return count ;
    }
}
